{
  "name": "alex-patterson-webdev/laminas-doctrine-fixtures",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "doctrine/orm": "^2.7",
    "doctrine/data-fixtures": "^1.3",
    "laminas/laminas-modulemanager": "^2.8",
    "laminas/laminas-eventmanager": "^3.2",
    "alex-patterson-webdev/laminas-factory": "^1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasDoctrineFixtures\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasDoctrineFixtures\\": "test/phpunit"
    }
  },
  "scripts": {
    "test": "phpunit --coverage-clover=coverage.xml"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
