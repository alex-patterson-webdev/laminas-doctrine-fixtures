{
  "name": "alex-patterson-webdev/laminas-doctrine-fixtures",
  "description": "Doctrine Data Fixtures integration module for the Laminas Framework",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.0",
    "doctrine/orm": "^2.7",
    "doctrine/data-fixtures": "^1.3",
    "laminas/laminas-modulemanager": "^2.8",
    "laminas/laminas-eventmanager": "^3.2",
    "alex-patterson-webdev/laminas-factory": "dev-feature/3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasDoctrineFixtures\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasDoctrineFixtures\\": "test/phpunit"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
